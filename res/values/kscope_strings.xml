    <!-- GMS switch -->
    <string name="gms_category">Google settings</string>
    <string name="gms_enabled">Enable Google services</string>
    <string name="gms_enabled_desc">Provides Google-related services and apps such as Google Play and Google Account. Turning this feature on is recommended, but it may increase battery usage.</string>
    <string name="gms_disable_alert_title">Notice</string>
    <string name="gms_disable_alert_message">You are trying to disable Google services. Once you\'ve enabled these services, it\'s strongly recommended to leave them enabled. Otherwise, it may lead to issues with your currently installed apps. Although this toggle doesn\'t touch any data, you\'ll still be automatically signed out of this device. Thus, make sure that "Find My Device" has been disabled before you proceed. And if necessary, disable 2-step verification on your Google account as well. Perform a factory reset yourself if you run into any issues. DO NOT SPAM FOR HELP.</string>
    <string name="gms_disable_alert_positive_btn">OK, I agree</string>
    <string name="gms_disable_alert_negative_btn">Cancel</string>

